<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane prefHeight="390.0" prefWidth="531.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.controller.WelcomeController">
   <children>
      <VBox alignment="CENTER" layoutX="31.0" layoutY="17.0" prefHeight="354.0" prefWidth="467.0" style="-fx-spacing: 20;">
         <children>
            <ImageView fx:id="imageView" fitHeight="200.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/three_cats_on_a_bench.jpeg.jpeg" />
               </image>
            </ImageView>
            <Label text="Welcome To The Bench!" textAlignment="CENTER" textFill="#f0f0f0">
               <font>
                  <Font name="Candara Bold" size="18.0" />
               </font>
            </Label>
            <HBox alignment="CENTER" prefHeight="39.0" prefWidth="467.0" style="-fx-spacing: 20;">
               <children>
                  <Button fx:id="loginBtn" mnemonicParsing="false" style="-fx-background-color: black; -fx-border-color: white; -fx-border-radius: 8;" text="Login" textFill="#ececec">
                     <font>
                        <Font name="Candara" size="15.0" />
                     </font>
                  </Button>
                  <Button fx:id="signupBtn" mnemonicParsing="false" style="-fx-background-color: black; -fx-border-color: white; -fx-border-radius: 8;" text="Sign up" textFill="#ececec">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</StackPane>
